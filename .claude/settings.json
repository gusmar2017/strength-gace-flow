{
  "model": "sonnet",
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(poetry:*)",
      "Bash(python:*)",
      "Bash(pip:*)",
      "Bash(git:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(rm:*)",
      "Bash(cd:*)",
      "Bash(touch:*)",
      "Bash(curl:*)",
      "Bash(uvicorn:*)",
      "Bash(source:*)",
      "Bash(which:*)",
      "Bash(pwd:*)",
      "Bash(chmod:*)",
      "Bash(pytest:*)",
      "Bash(black:*)",
      "Bash(isort:*)",
      "Bash(xcodebuild:*)",
      "Bash(swift:build)",
      "Bash(swift:test)",
      "Bash(railway:logs)",
      "Bash(railway:status)",
      "Bash(railway:list)",
      "Bash(railway:link)",
      "Bash(railway:variables)",
      "Bash(firebase:projects:*)",
      "Bash(firebase:use:*)",
      "Bash(firebase:functions:log)",
      "Bash(firebase:firestore:*)",
      "Read(*)",
      "Write(*)",
      "Edit(*)"
    ],
    "deny": [
      "Bash(git:push --force*)",
      "Bash(git:push -f*)",
      "Bash(git:reset --hard*)",
      "Bash(railway:up*)",
      "Bash(railway:deploy*)",
      "Bash(firebase:deploy*)",
      "Bash(firebase:functions:delete*)",
      "Bash(cat:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(echo:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(node:*)",
      "Bash(nvm:*)"
    ]
  },
  "env": {
    "ANTHROPIC_DEFAULT_OPUS_MODEL": "claude-opus-4-5-20251101",
    "ANTHROPIC_DEFAULT_SONNET_MODEL": "claude-sonnet-4-5-20250929",
    "ANTHROPIC_DEFAULT_HAIKU_MODEL": "claude-3-5-haiku-20241022"
  }
}
